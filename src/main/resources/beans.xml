<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="actorinfo" class="com.stackroute.domain.Actor">
    <property name="name" value="ram"/>
    <property name="gender" value="male"/>
    <property name="age" value="24"/>

</bean>
    <bean id="movie1" class="com.stackroute.domain.Movie" scope="prototype" autowire="byType">
        <property name="actor" ref="actorinfo"></property>

    </bean>
    <bean id="movie2" class="com.stackroute.domain.Movie" scope="prototype" autowire="byName">
        <property name="actor" ref="actorinfo"></property>

    </bean>
    <bean id="movie3" class="com.stackroute.domain.Movie" scope="prototype" autowire="constructor">
        <property name="actor" ref="actorinfo"></property>

    </bean>

</beans>